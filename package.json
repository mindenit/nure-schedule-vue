{
  "name": "nure-schedule",
  "description": "Schedule app for NURE university",
  "version": "0.0.0",
  "type": "module",
  "author": "Mindenit team",
  "homepage": "https://github.com/mindenit/nure-schedule-vue",
  "maintainers": [
    {
      "name": "Kyrylo Savieliev",
      "url": "https://github.com/OneLiL05"
    },
    {
      "name": "Roman Trashutin",
      "url": "https://github.com/perkinson1251"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mindenit/nure-schedule-vue.git"
  },
  "bugs": {
    "url": "https://github.com/OneLiL05/nurekit/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,vue}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.17.15",
    "@vueuse/core": "^10.7.1",
    "nurekit": "^0.4.3",
    "pinia": "^2.1.7",
    "radix-vue": "^1.3.0",
    "vue": "^3.4.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.25"
  },
  "private": true,
  "engines": {
    "node": ">=18.18.1"
  }
}
